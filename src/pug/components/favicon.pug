mixin favicon()
	link(rel='apple-touch-icon' sizes='152x152' href=pathPrefix +'favicon/apple-touch-icon.png')
	link(rel='icon' type='image/png' sizes='32x32' href=pathPrefix +'favicon/favicon-32x32.png')
	link(rel='icon' type='image/png' sizes='16x16' href=pathPrefix +'favicon/favicon-16x16.png')
	link(rel='manifest' href=pathPrefix +'favicon/site.webmanifest')
	link(rel='mask-icon' href=pathPrefix +'favicon/safari-pinned-tab.svg' color='#5bbad5')
	link(rel='shortcut icon' href=pathPrefix +'favicon/favicon.ico')
	meta(name='msapplication-TileColor' content='#da532c')
	meta(name='msapplication-config' content=pathPrefix +'favicon/browserconfig.xml')
	meta(name='theme-color' content='#ffffff')
